{
  "name": "@figureland/vite-plugin-git",
  "version": "0.0.8",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.js",
      "import": "./dist/index.js"
    },
    "./module": {
      "types": "./module.d.ts"
    }
  },
  "description": "A tiny plugin that makes git information available in your Vite app",
  "scripts": {
    "build": "bun run scripts/build.ts",
    "dev": "bun --watch scripts/dev.ts",
    "prepublishOnly": "bun run build",
    "prepare": "husky",
    "lint": "eslint src/",
    "format": "prettier --write src/",
    "release": "bunx changelogen@latest --release"
  },
  "files": [
    "dist",
    "module.d.ts"
  ],
  "license": "MIT",
  "devDependencies": {
    "@commitlint/cli": "^19.2.1",
    "@commitlint/config-conventional": "^19.1.0",
    "@types/bun": "^1.0.0",
    "bun-plugin-dts": "^0.2.1",
    "eslint": "^9.0.0",
    "husky": "^9.0.11",
    "prettier": "^3.2.5",
    "typescript": "^5.2.2",
    "typescript-eslint": "^7.6.0",
    "vite": "^5.2.11"
  },
  "peerDependencies": {
    "vite": ">5.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/figureland/vite-plugin-git.git"
  },
  "author": "Figure Systems",
  "bugs": {
    "url": "https://github.com/figureland/vite-plugin-git"
  },
  "homepage": "https://github.com/figureland/vite-plugin-git#readme"
}
